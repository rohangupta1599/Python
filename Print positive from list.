list=[]
print("There Are A Total Of 5 Number In List")
print("Input The Numbers For list 1")
a=int(input("Input The Numbers"))
list.append(a)
b=int(input("Input The Numbers"))
list.append(b)
c=int(input("Input The Numbers"))
list.append(c)
d=int(input("Input The Numbers"))
list.append(d)
e=int(input("Input The Numbers"))
list.append(e)
print("Input: list1 = ",list)
for num in list:
    if num >= 0:
        print(num, end=" ")

list1=[]
print("\n Input The Numbers For list 2")
a=int(input("Input The Numbers"))
list1.append(a)
b=int(input("Input The Numbers"))
list1.append(b)
c=int(input("Input The Numbers"))
list1.append(c)
d=int(input("Input The Numbers"))
list1.append(d)
e=int(input("Input The Numbers"))
list1.append(e)
print("Input: list2 = ",list1)
for num in list1:
    if num >= 0:
        print(num, end=" ")
